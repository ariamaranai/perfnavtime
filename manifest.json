{
  "manifest_version": 3,
  "name": "perfnavtime",
  "version": "0",
  "description": "Show PerformanceNavigationTiming",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "tabs",
    "userScripts"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.htm"
  }
}